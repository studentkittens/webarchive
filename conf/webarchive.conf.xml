<webarchive>
	<general>
		<root>/archive</root>
	</general>
	<!-- modules -->	
	<crawler> 
		<depth>5</depth>
		<tinterval>2:0:0</tinterval>
		<maxInst>10</maxInst>
		<userAgent>?</userAgent>
		<tempRoot>/tmp</tmpRoot>
		<filters>
			<filter name="mimeFilter">
				<srcPath>src/filter/mimeFilter.py</srcPath>
				<excludes>*</excludes>
				<includes>text/html</includes>
			</filter>
		</filters>
	</crawler>
</webarchive>
